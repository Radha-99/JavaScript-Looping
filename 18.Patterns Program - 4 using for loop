Print the following pattern using for loop


*    *
**   **
***  ***
**** ****
**********

function printPattern() {
  /* Function to print the pattern */
  for (let i = 0; i <= 4; i++) {
    let pattern = "";

    for (let j = 0; j <= i; j++) {
      pattern += "*";
    }

    for (let j = 0; j <= 3 - i; j++) {
      pattern += " ";
    }

    for (let j = 0; j <= i; j++) {
      pattern += "*";
    }

    console.log(pattern);
  }
}

/* Call the function */
printPattern();
